<template id="iteration-stats-template">
    <table class="iteration-stats">
        <tr class="iteration-name">
            <th colspan="2">Iteration</th>
        </tr>
        <tr class="total">
            <th class="title" rowspan="3">Finished Tasks</th>
            <th>Total</th>
        </tr>
        <tr class="defects">
            <th>Defects</th>
        </tr>
        <tr class="success">
            <th>Success</th>
        </tr>
        <tr class="wip">
            <th colspan="2">WIP (tasks)</th>
        </tr>
        <tr class="throughput">
            <th colspan="2">Throughput (tasks/min)</th>
        </tr>
        <tr class="lead-time">
            <th colspan="2">Lead time (min)</th>
        </tr>
    </table>
</template>
