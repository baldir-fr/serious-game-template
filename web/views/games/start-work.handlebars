<div class="container" data-game-id="{{work.gameId}}">
    <h1>{{work.work.title}}</h1>

    <div>
        <progress value="" id="progressbar"></progress>
    </div>

    <div id="work-container">
        <div id="workspace">
            <h2>Workspace</h2>
            <button id="create-task" disabled="disabled">Create Task</button>
        </div>

        <div id="outbox" class="queue-column" data-column-id="{{work.work.columnId}}">
            <h2>Outbox</h2>
            <div class="tasks"></div>
        </div>
    </div>

</div>

{{>cardtemplate }}